<view class="goods_detail"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="true"><block wx:for="{{goodsInfo.pics}}" wx:for-item="item" wx:for-index="i" wx:key="pics_id"><swiper-item><image src="{{item.pics_mid_url}}" mode data-event-opts="{{[['tap',[['previewImg',[i]]]]]}}" bindtap="__e"></image></swiper-item></block></swiper><block wx:if="{{goodsInfo.goods_name}}"><view class="goods-info-box"><view class="price">{{"￥"+goodsInfo.goods_price}}</view><view class="goods-info-body"><view class="goods-name">{{goodsInfo.goods_name}}</view><view class="favi"><van-icon vue-id="c64ca100-1" name="star-o" size="20px" data-com-type="wx" bind:__l="__l"></van-icon><text>收藏</text></view></view><view class="yf">快递：免运费</view></view></block><rich-text nodes="{{goodsInfo.goods_introduce}}"></rich-text><van-goods-action vue-id="c64ca100-2" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-goods-action-icon vue-id="{{('c64ca100-3')+','+('c64ca100-2')}}" icon="chat-o" text="客服" bind:click="onClickIcon" data-com-type="wx" bind:__l="__l"></van-goods-action-icon><van-goods-action-icon vue-id="{{('c64ca100-4')+','+('c64ca100-2')}}" icon="cart-o" text="购物车" bind:click="onClickIcon" data-com-type="wx" bind:__l="__l"></van-goods-action-icon><van-goods-action-button vue-id="{{('c64ca100-5')+','+('c64ca100-2')}}" text="加入购物车" type="warning" data-event-opts="{{[['^click',[['addToCart']]]]}}" data-com-type="wx" bind:click="__e" bind:__l="__l"></van-goods-action-button><van-goods-action-button vue-id="{{('c64ca100-6')+','+('c64ca100-2')}}" text="立即购买" data-event-opts="{{[['^click',[['goCart']]]]}}" data-com-type="wx" bind:click="__e" bind:__l="__l"></van-goods-action-button></van-goods-action></view>